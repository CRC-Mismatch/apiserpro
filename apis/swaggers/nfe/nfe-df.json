                                      "example": "07"
                                    }
                                  }
                                }
                              }
                            },
                            "PIS": {
                              "type": "object",
                              "properties": {
                                "PISNT": {
                                  "type": "object",
                                  "properties": {
                                    "CST": {
                                      "type": "integer",
                                      "example": "07"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  },
                  "total": {
                    "type": "object",
                    "properties": {
                      "ICMSTot": {
                        "type": "object",
                        "properties": {
                          "vCONFINS": {
                            "type": "number",
                            "example": 0
                          },
                          "vBCST": {
                            "type": "number",
                            "example": 0
                          },
                          "vICMSDeson": {
                            "type": "number",
                            "example": 0
                          },
                          "vProd": {
                            "type": "number",
                            "example": 265.79
                          },
                          "vSeq": {
                            "type": "number",
                            "example": 0
                          },
                          "vFCP": {
                            "type": "number",
                            "example": 0
                          },
                          "vFCPST": {
                            "type": "number",
                            "example": 0
                          },
                          "vNF": {
                            "type": "number",
                            "example": 0
